<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:layout_marginHorizontal="@dimen/margin_16"
    android:orientation="vertical">

    <TextView
        style="@style/text_medium_22"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginBottom="@dimen/margin_16"
        android:paddingVertical="@dimen/margin_19"
        android:text="@string/navigation_Team" />

    <TextView
        style="@style/text_bold_32"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginBottom="@dimen/margin_24"
        android:paddingVertical="@dimen/margin_8"
        android:text="@string/team_Who_works" />

    <TextView
        style="@style/text_medium_16"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginBottom="@dimen/margin_16"
        android:text="@string/team_Ekaterina" />

    <TextView
        style="@style/text_medium_16"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginBottom="@dimen/margin_16"
        android:text="@string/team_Tatyana" />

    <TextView
        style="@style/text_medium_16"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginBottom="@dimen/margin_16"
        android:text="@string/team_Dimitrii" />

    <TextView
        style="@style/text_medium_16"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="@string/team_Emil" />

</LinearLayout>
